{
  "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.inlineSuggest.enabled": true,
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.wordWrap": "on",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": false,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "editor.mouseWheelZoom": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorWidth": 3,
  "editor.wordWrapColumn": 90,
  "editor.cursorBlinking": "expand",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.renderLineHighlight": "all",
  "editor.renderControlCharacters": false,
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.indentation": false,
  "editor.guides.bracketPairs": true,
  "editor.hover.enabled": true,
  "editor.codeLens": true,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.letterSpacing": 0.3,
  "editor.renderWhitespace": "none",
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.lineHeight": 23,
  "files.associations": {
    "*.json": "jsonc",
    "*.js": "javascriptreact"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.language": "en,vi",
  "files.eol": "\n",
  "files.exclude": {
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/*.js": { "when": "$(basename).ts" }
  },
  // Rainbow

  "indentRainbow.colors": [
    "rgba(247, 37, 133,0.07)",
    "rgba(181, 23, 158,0.07)",
    "rgba(114, 9, 183,0.07)",
    "rgba(86, 11, 173,0.07)",
    "rgba(72, 12, 168,0.07)",
    "rgba(58, 12, 163,0.07)",
    "rgba(63, 55, 201,0.07)",
    "rgba(67, 97, 238,0.07)",
    "rgba(72, 149, 239,0.07)",
    "rgba(76, 201, 240,0.07)"
  ],
  "indentRainbow.ignoreLinePatterns": ["/[ \t]* [*]/g", "/[ \t]+[/]{2}/g"],
  "indentRainbow.errorColor": "rgba(rgba(255, 0, 0, 0.4)",
  //Beautify
  "beautify.ignore": ["*/test.js", "**/spec/*", "**/test/**/*"],
  "beautify.language": {
    "js": {
      "type": ["javascript", "json"],
      "filename": [".jshintrc", ".jsbeautifyrc"]
    },
    "css": ["css", "scss"],
    "html": ["html", "php", "erb", "htm"]
  },
  "kite.showWelcomeNotificationOnStartup": false,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.eslintIntegration": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": true,
  "window.closeWhenEmpty": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "files.trimTrailingWhitespace": true,
  "emmet.triggerExpansionOnTab": true,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
"workbench.colorTheme": "Aura Soft Dark",
  "settingsSync.ignoredExtensions": [
    "github.copilot",
    "ms-azuretools.vscode-docker",
    "adpyke.codesnap",
    "aaron-bond.better-comments",
    "akamud.vscode-theme-onelight",
    "davidanson.vscode-markdownlint"
  ],
  "git.confirmSync": false,
  "workbench.startupEditor": "none",
  "tabnine.experimentalAutoImports": true
}