{
  // Gitlend
  "gitlens.defaultDateLocale": "en-vi",
  "gitlens.defaultDateFormat": "MMM:DD:YYYY h:m",
  "gitlens.defaultTimeFormat": "h:mm",
  "gitlens.defaultDateShortFormat": null,
  "gitlens.hovers.currentLine.over": "line",
  // Code runner
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.saveAllFilesBeforeRun": true,
  //   Terminal
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontFamily": "Fira Code, Roboto Mono, monospace",
  "terminal.integrated.fontWeight": "500",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.profiles.osx": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh"
    },
    "fish": {
      "path": "fish"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Ubuntu": {
      "path": "C:\\Windows\\System32\\wsl.exe",
      "args": []
    },
    "Ubuntu (WSL)": {
      "path": "C:\\Windows\\System32\\wsl.exe",
      "args": ["-d", "Ubuntu"]
    }
  },
  "[javascript, javascriptreact, typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json, jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  //   More

  "atlascode.jira.jqlList": [
    {
      "id": "32f02d69-b404-4d5a-af19-25944d6c391a",
      "enabled": true,
      "name": "My duytruong23 Issues",
      "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
      "siteId": "45dc59b0-e734-4bdd-a6cf-cc3602a32eb6",
      "monitor": true
    }
  ],
  "atlascode.helpExplorerEnabled": false,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "yaml.schemas": {
    "file:///c%3A/Users/truon/.vscode/extensions/atlassian.atlascode-2.10.11/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "redhat.telemetry.enabled": true
}
