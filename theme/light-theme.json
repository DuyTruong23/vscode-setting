{
  "[Visual Studio Light]": {
    "sideBar.background": "#F2F1F8",
    "sideBar.foreground": "#8d92ff",
    "editor.background": "#F2F1F8",
    "editorWidget.border": "#8d92ff",
    "editorWidget.background": "#F2F1F8",
    "editorCursor.foreground": "#9a86fd",
    "activityBar.background": "#F2F1F8",
    "activityBar.foreground": "#9a86fd",
    "activityBar.inactiveForeground": "#C7C7CC",
    "tab.activeBorderTop": "#9a86fd",
    "tab.inactiveBackground": "#F2F1F8",
    "tab.activeBackground": "#E2DFF6",
    "panel.background": "#F2F1F8",
    "panelTitle.activeBorder": "#8d92ff",
    "panel.border": "#8d92ff",
    "statusBar.background": "#E2DFF6",
    "statusBar.foreground": "#8d92ff",
    "activityBar.activeBorder": "#9a86fd",
    "activityBarBadge.background": "#8E80FF",
    "sideBarSectionHeader.background": "#F2F1F8",
    "sideBarSectionHeader.border": "#9a86fd",
    "terminal.background": "#F2F1F8",
    "terminal.foreground": "#8E8E93",
    "terminal.ansiCyan": "#32ADE6",
    "terminal.ansiGreen": "#34C759",
    "terminal.ansiYellow": "#FFCC00",
    "terminal.ansiMagenta": "#FF3B30",
    "titleBar.activeBackground": "#EDECF8",
    "breadcrumb.background": "#E2DFF6"
  },
  "[Visual Studio Light]": {
    "textMateRules": [
      {
        "scope": ["entity.name.tag"],
        "settings": {
          "foreground": "#FF3B30"
        }
      },
      {
        "scope": [
          "entity.other.attribute-name",
          "entity.other.attribute-name.id.css",
          "storage.type",
          "keyword.control.flow.js",
          "keyword.control.conditional.js",
          "keyword.operator"
        ],
        "settings": {
          "foreground": "#AF52DE",
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "string.quoted.double.html",
          "string.quoted.single.html",
          "string.quoted.double.js",
          "string.quoted.single.js",
          "punctuation.definition.entity.css",
          "string"
        ],
        "settings": {
          "foreground": "#34C759"
        }
      },
      {
        "scope": [
          "punctuation.definition.tag",
          "keyword.control",
          "support.class.js"
        ],
        "settings": {
          "foreground": "#30B0C7"
        }
      },
      {
        "scope": [
          "support.type.property-name",
          "punctuation.separator.comma.js",
          "punctuation.terminator.statement.js"
        ],
        "settings": {
          "foreground": "#007AFF"
        }
      },
      {
        "scope": ["comment", "punctuation.definition.comment.html"],
        "settings": {
          "foreground": "#AABFC9"
        }
      },
      {
        "scope": [
          "text.html.derivative",
          "meta.object-literal.key.js",
          "variable.other.property.js",
          "variable.other.readwrite.js",
          "variable.other.object.js",
          "string.regexp",
          "variable.other.object.property.js",
          "support.variable.property.js"
        ],
        "settings": {
          "foreground": "#546E7A"
        }
      },
      {
        "scope": ["entity.other.attribute-name.class.css"],
        "settings": {
          "foreground": "#FFCC00"
        }
      },
      {
        "scope": [
          "constant.numeric",
          "keyword.other.unit",
          "entity.name.function.js",
          "constant.language",
          "variable.language"
        ],
        "settings": {
          "foreground": "#FF9500"
        }
      }
    ]
  }
}
