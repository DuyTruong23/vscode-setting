{
  "editor.inlineSuggest.enabled": true,
  "editor.suggestSelection": "first",
  "editor.codeLens": true,
  "editor.renderControlCharacters": false,
  "editor.guides.indentation": false,
  "editor.renderLineHighlight": "all",
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "gitlens.defaultDateLocale": "en-vi",
  "gitlens.defaultDateFormat": "MMM:DD:YYYY h:m",
  "gitlens.defaultTimeFormat": "h:mm",
  "gitlens.defaultDateShortFormat": null,
  "gitlens.hovers.currentLine.over": "line",
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "workbench.tree.renderIndentGuides": "none",
  "liveServer.settings.donotShowInfoMsg": true,
  "beautify.ignore": ["*/test.js", "**/spec/*", "**/test/**/*"],
  "diffEditor.codeLens": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript, javascriptreact, typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json, jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "cSpell.diagnosticLevel": "Hint",
  "files.associations": {
    "*.json": "jsonc"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/*.js": { "when": "$(basename).ts" }
  },
  //! Add on
  "atlascode.jira.jqlList": [
    {
      "id": "32f02d69-b404-4d5a-af19-25944d6c391a",
      "enabled": true,
      "name": "My duytruong23 Issues",
      "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
      "siteId": "45dc59b0-e734-4bdd-a6cf-cc3602a32eb6",
      "monitor": true
    }
  ],
  "atlascode.helpExplorerEnabled": false,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "yaml.schemas": {
    "file:///c%3A/Users/truon/.vscode/extensions/atlassian.atlascode-2.10.11/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "redhat.telemetry.enabled": true
}
